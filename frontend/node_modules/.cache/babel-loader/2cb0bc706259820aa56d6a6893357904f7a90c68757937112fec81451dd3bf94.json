rd class=\"stat-card\">\r\n          <div class=\"card-content\">\r\n            <div class=\"card-icon bg-blue\">\r\n              <i class=\"el-icon-user-solid\"></i>\r\n            </div>\r\n            <div class=\"card-info\">\r\n              <div class=\"card-title\">参保人数</div>\r\n              <div class=\"card-value\">1,234,567</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <!-- 其他统计卡片 -->\r\n    </el-row>\r\n\r\n    <el-card class=\"chart-card\">\r\n      <template #header>\r\n        <div class=\"chart-title\">\r\n          <span>医保费用分布</span>\r\n          <el-date-picker\r\n            v-model=\"chartDate\"\r\n            type=\"month\"\r\n            placeholder=\"选择月份\">\r\n          </el-date-picker>\r\n        </div>\r\n      </template>\r\n      <div class=\"chart-container\">\r\n        <v-chart :option=\"chartOptions\" autoresize/>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { use } from 'echarts/core'\r\nimport { PieChart } from 'echarts/charts'\r\nimport { CanvasRenderer } from 'echarts/renderers'\r\nimport VChart from 'vue-echarts'\r\n\r\nuse([PieChart, CanvasRenderer])\r\n\r\nexport default {\r\n  components: {\r\n    VChart\r\n  },\r\n  data() {\r\n    return {\r\n      chartDate: new Date(),\r\n      chartOptions: {\r\n        tooltip: { trigger: 'item' },\r\n        series: [{\r\n          type: 'pie',\r\n          data: [\r\n            { value: 335, name: '药品费用' },\r\n            { value: 310, name: '诊疗费用' },\r\n            { value: 234, name: '材料费用' }\r\n          ]\r\n        }]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.$axios.get('/api/health-check')\r\n      .then(response => {\r\n        if (response.data.databaseStatus !== 'active') {\r\n          this.$message.error('数据库连接异常');\r\n          this.$router.push('/maintenance');\r\n          this.$axios.post('/api/db-recovery', { \r\n            logFile: 'binlog.000219',\r\n    