arch = function handleSearch() {\n      ElMessage.success('触发搜索');\n      // 实际应用中这里会调用API\n    };\n    var resetSearch = function resetSearch() {\n      Object.keys(searchForm).forEach(function (key) {\n        if (key === 'dateRange') {\n          searchForm[key] = [];\n        } else {\n          searchForm[key] = '';\n        }\n      });\n    };\n\n    // 分页功能\n    var handleSizeChange = function handleSizeChange(val) {\n      pageSize.value = val;\n      // 实际应用中这里会刷新数据\n    };\n    var handleCurrentChange = function handleCurrentChange(val) {\n      currentPage.value = val;\n      // 实际应用中这里会刷新数据\n    };\n\n    // 操作功能\n    var viewDetail = function viewDetail(row) {\n      ElMessage.info(\"\\u67E5\\u770B\\u5C31\\u8BCA\\u8BB0\\u5F55: \".concat(row.mdtrtId));\n      // 实际应用中这里会跳转到详情页\n    };\n    var auditRecord = function auditRecord(row) {\n      ElMessage.info(\"\\u5BA1\\u6838\\u5C31\\u8BCA\\u8BB0\\u5F55: \".concat(row.mdtrtId));\n      // 实际应用中这里会打开审核对话框\n    };\n    var __returned__ = {\n      searchForm: searchForm,\n      currentPage: currentPage,\n      pageSize: pageSize,\n      total: total,\n      auditList: auditList,\n      getStatusType: getStatusType,\n      getStatusText: getStatusText,\n      handleSearch: handleSearch,\n      resetSearch: resetSearch,\n      handleSizeChange: handleSizeChange,\n      handleCurrentChange: handleCurrentChange,\n      viewDetail: viewDetail,\n      auditRecord: auditRecord,\n      ref: ref,\n      reactive: reactive,\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","ElMessage","searchForm","mdtrtId","status","dateRange","currentPage","pageSize","total","auditList","patientName","visitDate","deptName","diagnosisName","totalFee","abnormalItems","getStatusType","getStatusText","handleSearch","success","resetSearch","Object","keys","forEach","key","handleSizeChange","val","value","handleCurrentChange","viewDetail","row","info","concat","auditRecord"],"sources":["D:/Code/Project/SSR/frontend/src/views/OutpatientAudit.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <el-card shadow=\"hover\">\r\n      <template #header>\r\n        <div class=\"card-header\">\r\n          <span class=\"title\">门诊智能审核</span>\r\n          <el-tag type=\"success\">AI辅助</el-tag>\r\n        </div>\r\n      </template>\r\n      \r\n      <el-form :inline=\"true\" :model=\"searchForm\">\r\n        <el-form-item label=\"就诊编号\">\r\n          <el-input v-model=\"searchForm.mdtrtId\" placeholder=\"请输入就诊编号\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"审核状态\">\r\n          <el-select v-model=\"searchForm.status\" placeholder=\"请选择\">\r\n            <el-option label=\"全部\" value=\"\" />\r\n            <el-option label=\"待审核\" value=\"pending\" />\r\n            <el-option label=\"异常\" value=\"abnormal\" />\r\n            <el-option label=\"已通过\" value=\"passed\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"日期范围\">\r\n          <el-date-picker\r\n            v-model=\"searchForm.dateRange\"\r\n            type=\"daterange\"\r\n            range-separator=\"至\"\r\n            start-placeholder=\"开始日期\"\r\n            end-placeholder=\"结束日期\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"handleSearch\">搜索</el-button>\r\n          <el-button @click=\"resetSearch\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n      \r\n      <el-table :data=\"auditList\" border stripe style=\"width: 100%\">\r\n        <el-table-column prop=\"mdtrtId\" label=\"就诊编号\" width=\"180\" />\r\n        <el-table-column prop=\"patientName\" label=\"患者姓名\" width=\"120\" />\r\n        <el-table-column prop=\"visitDate\" label=\"就诊日期\" width=\"120\" />\r\n        <el-table-column prop=\"deptName\" label=\"科室\" width=\"120\" />\r\n        <el-table-column prop=\"diagnosisName\" label=\"诊断\" />\r\n        <el-table-column prop=\"totalFee\" label=\"总费用\" width=\"120\" align=\"right\" />\r\n        <el-table-column prop=\"abnormalItems\" label=\"异常项数\" width=\"100\" align=\"center\">\r\n          <template #default=\"scope\">\r\n            <el-tag v-if=\"scope.row.abnormalItems > 0\" type=\"danger\">{{ scope.row.abnormalItems }}</el-tag>\r\n            <span v-else>0</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"status\" label=\"审核状态\" width=\"100\">\r\n          <template #default=\"scope\">\r\n            <el-tag :type=\"getStatusType(scope.row.status)\">{{ getStatusText(scope.row.status) }}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"150\" fixed=\"right\">\r\n          <template #default=\"scope\">\r\n            <el-button link type=\"primary\" @click=\"viewDetail(scope.row)\">查看</el-button>\r\n            <el-button link type=\"primary\" @click=\"auditRecord(scope.row)\">审核</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      \r\n      <el-pagination\r\n        v-model:current-page=\"currentPage\"\r\n        v-model:page-size=\"pageSize\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        class=\"pagination\"\r\n      />\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive } from 'vue'\r\nimport { ElMessage } from 'element-plus'\r\n\r\n// 搜索表单\r\nconst searchForm = reactive({\r\n  mdtrtId: '',\r\n  status: '',\r\n  dateRange: []\r\n})\r\n\r\n// 分页\r\nconst currentPage = ref(1)\r\nconst pageSize = ref(10)\r\nconst total = ref(