tem.label\" :value=\"item.value\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"日期范围\">\r\n          <el-date-picker\r\n            v-model=\"dateRange\"\r\n            type=\"daterange\"\r\n            range-separator=\"至\"\r\n            start-placeholder=\"开始日期\"\r\n            end-placeholder=\"结束日期\"\r\n            format=\"YYYY-MM-DD\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" :icon=\"Search\" @click=\"handleSearch\">查询</el-button>\r\n          <el-button :icon=\"Refresh\" @click=\"resetQuery\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <!-- 数据表格 -->\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"doctorList\"\r\n        border\r\n        stripe\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-table-column prop=\"doctorId\" label=\"医生编号\" width=\"120\" />\r\n        <el-table-column prop=\"hospitalCode\" label=\"医疗机构编号\" width=\"150\" />\r\n        <el-table-column prop=\"hospitalName\" label=\"医疗机构名称\" width=\"180\" show-overflow-tooltip />\r\n        <el-table-column prop=\"department\" label=\"所属科室\" width=\"120\" />\r\n        <el-table-column prop=\"community\" label=\"所属社区\" width=\"150\" show-overflow-tooltip />\r\n        <el-table-column prop=\"prescriptionCount\" label=\"处方数量\" width=\"100\" align=\"right\" sortable />\r\n        <el-table-column prop=\"totalCost\" label=\"总花费(元)\" width=\"120\" align=\"right\" sortable>\r\n          <template #default=\"scope\">\r\n            <span>{{ formatCurrency(scope.row.totalCost) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"avgCostPerPrescription\" label=\"平均处方费用(元)\" width=\"150\" align=\"right\" sortable>\r\n          <template #default=\"scope\">\r\n            <span>{{ formatCurrency(scope.row.avgCostPerPrescription) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"lastPrescriptionDate\" label=\"最近开方日期\" width=\"150\" />\r\n        <el-table-column prop=\"riskScore\" label=\"风险评分\" width=\"100\" align=\"center\" sortable>\r\n          <template #default=\"scope\">\r\n            <el-tag :type=\"getRiskTagType(scope.row.riskScore)\">\r\n              {{ scope.row.riskScore }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180\" fixed=\"right\">\r\n          <template #default=\"scope\">\r\n            <el-button type=\"primary\" link @click=\"handleRiskAssessment(scope.row)\">风险评估</el-button>\r\n            <el-button type=\"warning\" link @click=\"handleManualReview(scope.row)\">人工审核</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页器 -->\r\n      <el-pagination\r\n        v-model:current-page=\"currentPage\"\r\n        v-model:page-size=\"pageSize\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        @current-change=\"handlePageChange\"\r\n        @size-change=\"handleSizeChange\"\r\n        class=\"pagination\"\r\n      />\r\n    </el-card>\r\n\r\n    <!-- 风险评估对话框 -->\r\n    <el-dialog v-model=\"riskDialogVisible\" title=\"风险评估\" width=\"65%\" destroy-on-close>\r\n      <div v-if=\"selectedDoctor\" class=\"risk-assessment\">\r\n        <div class=\"doctor-info\">\r\n          <h3>{{ selectedDoctor.hospitalName }} - {{ selectedDoctor.doctorName || '医生' }}</h3>\r\n          <p>科室: {{ selectedDoctor.department }} | 处方数量: {{ selectedDoctor.prescriptionCount }} | 总费用: {{ formatCurrency(selectedDoctor.totalCost) }}元</p>\r\n        </div>\r\n        \r\n        <el-divider content-position=\"center\">风险指标</el-divider>\r\n        \r\n        <div class=\"risk-indicators\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"8\">\r\n              <el-card shadow=\"hover\" :body-style=\"{ padding: '15px' }\">\r\n                <template #header>\r\n                  <div class=\"indicator-header\">\r\n                    <span>费用异常指数</span>\r\n                    <el-tag :type=\"getRiskTagType(riskData.costAnomalyScore)\">{{ riskData.costAnomalyScore }}</el-tag>\r\n                  </div>\r\n                </template>\r\n                <div class=\"indicator-body\">\r\n                  <el-progress :percentage=\"riskData.costAnomalyScore\" :color=\"getProgressColor(riskData.costAnomalyScore)\" />\r\n                  <p>高于同行业平均水平 {{ riskData.costDifference }}%</p>\r\n                </div>\r\n              </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n              <el-card shadow=\"hover\" :body-style=\"{ padding: '15px' }\">\r\n                <template #header>\r\n                  <div class=\"indicator-header\">\r\n                    <span>开方频率指数</span>\r\n                    <el-tag :type=\"getRiskTagType(riskData.prescriptionFrequencyScore)\">{{ riskData.prescriptionFrequencyScore }}</el-tag>\r\n                  </div>\r\n                </template>\r\n                <div class=\"indicator-body\">\r\n                  <el-progress :percentage=\"riskData.prescriptionFrequencyScore\" :color=\"getProgressColor(riskData.prescriptionFrequencyScore)\" />\r\n                  <p>每月平均开方 {{ riskData.monthlyPrescriptions }} 次</p>\r\n                </div>\r\n              </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n              <el-card shadow=\"hover\" :body-style=\"{ padding: '15px' }\">\r\n                <template #header>\r\n                  <div class=\"indicator-header\">\r\n                    <span>药品重复指数</span>\r\n                    <el-tag :type=\"getRiskTagType(riskData.drugRepetitionScore)\">{{ riskData.drugRepetitionScore }}</el-tag>\r\n                  </div>\r\n                </template>\r\n                <div class=\"indicator-body\">\r\n                  <el-progress :percentage=\"riskData.drugRepetitionScore\" :color=\"getProgressColor(riskData.drugRepetitionScore)\" />\r\n                  <p>重复开药占比 {{ riskData.repetitionRate }}%</p>\r\n                </div>\r\n              </el-card>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n        \r\n        <el-divider content-position=\"center\">风险趋势</el-divider>\r\n        \r\n        <div class=\"risk-trend\" ref=\"trendChart\"></div>\r\n        \r\n        <el-divider content-position=\"center\">异常处方列表</el-divider>\r\n        \r\n        <el-table :data=\"riskData.anomalyPrescriptions\" border stripe style=\"width: 100%\">\r\n          <el-table-column prop=\"prescriptionId\" label=\"处方ID\" width=\"120\" />\r\n          <el-table-column prop=\"patientId\" label=\"患者ID\" width=\"120\" />\r\n          <el-table-column prop=\"prescriptionDate\" label=\"开方日期\" width=\"150\" />\r\n          <el-table-column prop=\"totalCost\" label=\"金额(元)\" width=\"120\" align=\"right\" />\r\n          <el-table-column prop=\"anomalyType\" label=\"异常类型\" width=\"150\">\r\n            <template #default=\"scope\">\r\n              <el-tag :type=\"scope.row.anomalyType === '重复用药' ? 'danger' : scope.row.anomalyType === '超量开药' ? 'warning' : 'info'\">\r\n                {{ scope.row.anomalyType }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"confidence\" label=\"置信度\" width=\"120\">\r\n            <template #default=\"scope\">\r\n              <el-progress :percentage=\"scope.row.confidence * 100\" :color=\"getProgressColor(scope.row.confidence * 100)\" />\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"description\" label=\"异常描述\" show-overflow-tooltip />\r\n        </el-table>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, onMounted } from 'vue'\r\nimport { Search, Refresh } from '@element-plus/icons-vue'\r\nimport { ElMessage } from 'element-plus'\r\nimport * as echarts from 'echarts'\r\n\r\n// 页面数据\r\nconst loading = ref(false)\r\nconst currentPage = ref(1)\r\nconst pageSize = ref(10)\r\nconst total = ref(0)\r\nconst dateRange = ref([])\r\nconst doctorList = ref([])\r\nconst trendChartInstance = ref(null)\r\n\r\n// 风险评估数据\r\nconst riskDialogVisible = ref(false)\r\nconst selectedDoctor = ref(null)\r\nconst riskData = reactive({\r\n  costAnomalyScore: 75,\r\n  costDifference: 32,\r\n  prescriptionFrequencyScore: 65,\r\n  monthlyPrescriptions: 42,\r\n  drugRepetitionScore: 85,\r\n  repetitionRate: 38,\r\n  anomalyPrescriptions: []\r\n})\r\n\r\n// 查询参数\r\nconst queryParams = reactive({\r\n  doctorId: '',\r\n  hospital: '',\r\n  department: '',\r\n  community: '',\r\n  startDate: '',\r\n  endDate: ''\r\n})\r\n\r\n// 选项数据\r\nconst hospitalOptions = [\r\n  { value: 'H001', label: '第一人民医院' },\r\n  { value: 'H002', label: '中心医院' },\r\n  { value: 'H003', label: '妇幼保健院' },\r\n  { value: 'H004', label: '社区医疗中心' }\r\n]\r\n\r\nconst departmentOptions = [\r\n  { value: 'D001', label: '内科' },\r\n  { value: 'D002', label: '外科' },\r\n  { value: 'D003', label: '妇科' },\r\n  { value: 'D004', label: '儿科' },\r\n  { value: 'D005', label: '骨科' },\r\n  { value: 'D006', label: '神经科' }\r\n]\r\n\r\n// 初始化\r\nonMounted(() => {\r\n  fetchDoctorData()\r\n})\r\n\r\n// 获取医生数据\r\nconst fetchDoctorData = async () => {\r\n  loading.value = true\r\n  try {\r\n    // 模拟API调用\r\n    setTimeout(() => {\r\n      doctorList.value = generateMockDoctorData()\r\n      total.value = 156\r\n      loading.value = false\r\n    }, 800)\r\n  } catch (error) {\r\n    ElMessage.error('获取数据失败')\r\n    loading.value = false\r\n  }\r\n}\r\n\r\n// 生成模拟数据\r\nconst generateMockDoctorData = () => {\r\n  const mockData = []\r\n  for (let i = 1; i <= 10; i++) {\r\n    const hospitalIndex = Math.floor(Math.random() * 4)\r\n    const departmentIndex = Math.floor(Math.random() * 6)\r\n    const prescriptionCount = Math.floor(Math.random() * 200) + 50\r\n    const totalCost = Math.random() * 100000 + 10000\r\n    \r\n    mockData.push({\r\n      doctorId: 'DOC' + String(i).padStart(4, '0'),\r\n      doctorName: '张医生' + i,\r\n      hospitalCode: hospitalOptions[hospitalIndex].value,\r\n      hospitalName: hospitalOptions[hospitalIndex].label,\r\n      department: departmentOptions[departmentIndex].label,\r\n      community: '社区' + (Math.floor(Math.random() * 5) + 1),\r\n      prescriptionCount: prescriptionCount,\r\n      totalCost: totalCost,\r\n      avgCostPerPrescription: totalCost / prescriptionCount,\r\n      lastPrescriptionDate: '2023-12-' + String(Math.floor(Math.random() * 30) + 1).padStart(2, '0'),\r\n      riskScore: Math.floor(Math.random() * 100)\r\n    })\r\n  }\r\n  return mockData\r\n}\r\n\r\n// 处理风险评估\r\nconst handleRiskAssessment = (doctor) => {\r\n  selectedDoctor.value = doctor\r\n  riskDialogVisible.value = true\r\n  \r\n  // 模拟异常处方数据\r\n  riskData.anomalyPrescriptions = []\r\n  const anomalyTypes = ['重复用药', '超量开药', '不合理用药', '过度检查']\r\n  \r\n  for (let i = 1; i <= 5; i++) {\r\n    const type = anomalyTypes[Math.floor(Math.random() * anomalyTypes.length)]\r\n    riskData.anomalyPrescriptions.push({\r\n      prescriptionId: 'P' + String(Math.floor(Math.random() * 10000)).padStart(5, '0'),\r\n      patientId: 'PAT' + String(Math.floor(Math.random() * 10000)).padStart(5, '0'),\r\n      prescriptionDate: '2023-12-' + String(Math.floor(Math.random() * 30) + 1).padStart(2, '0'),\r\n      totalCost: Math.random() * 1000 + 100,\r\n      anomalyType: type,\r\n      confidence: Math.random() * 0.5 + 0.5,\r\n      description: type + '：医生在短期内为同一患者重复开具相同或类似药品，超出合理用量范围。'\r\n    })\r\n  }\r\n  \r\n  // 初始化趋势图表\r\n  setTimeout(() => {\r\n    initTrendChart()\r\n  }, 100)\r\n}\r\n\r\n// 初始化趋势图\r\nconst initTrendChart = () => {\r\n  const chartDom = document.querySelector('.risk-trend')\r\n  if (!chartDom) return\r\n  \r\n  trendChartInstance.value = echarts.init(chartDom)\r\n  \r\n  const months = ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月']\r\n  const riskScores = Array(12).fill(0).map(() => Math.floor(Math.random() * 40) + 60)\r\n  const avgScores = Array(12).fill(0).map(() => Math.floor(Math.random() * 20) + 40)\r\n  \r\n  const option = {\r\n    title: {\r\n      text: '医生风险评分趋势',\r\n      left: 'center'\r\n    },\r\n    tooltip: {\r\n      trigger: 'axis',\r\n      axisPointer: {\r\n        type: 'shadow'\r\n      }\r\n    },\r\n    legend: {\r\n      data: ['风险评分', '同科室平均'],\r\n      bottom: '0%'\r\n    },\r\n    grid: {\r\n      left: '3%',\r\n      right: '4%',\r\n      bottom: '10%',\r\n      top: '15%',\r\n      containLabel: true\r\n    },\r\n    xAxis: {\r\n      type: 'category',\r\n      data: months\r\n    },\r\n    yAxis: {\r\n      type: 'value',\r\n      min: 0,\r\n      max: 100\r\n    },\r\n    series: [\r\n      {\r\n        name: '风险评分',\r\n        type: 'line',\r\n        data: riskScores,\r\n        itemStyle: {\r\n          color: '#F56C6C'\r\n        },\r\n        lineStyle: {\r\n          width: 3\r\n        }\r\n      },\r\n      {\r\n        name: '同科室平均',\r\n        type: 'line',\r\n        data: avgScores,\r\n        itemStyle: {\r\n          color: '#409EFF'\r\n        },\r\n        lineStyle: {\r\n          width: 3,\r\n          type: 'dashed'\r\n        }\r\n      }\r\n    ]\r\n  }\r\n  \r\n  trendChartInstance.value.setOption(option)\r\n}\r\n\r\n// 处理人工审核\r\nconst handleManualReview = (doctor) => {\r\n  ElMessage.info(`正在准备${doctor.doctorName || doctor.doctorId}的人工审核表单...`)\r\n  // 这里可以跳转到详细的审核页面或打开审核对话框\r\n}\r\n\r\n// 查询处理\r\nconst handleSearch = () => {\r\n  if (dateRange.value && dateRange.value.length === 2) {\r\n    queryParams.startDate = dateRange.value[0]\r\n    queryParams.endDate = dateRange.value[1]\r\n  }\r\n  \r\n  currentPage.value = 1\r\n  fetchDoctorData()\r\n}\r\n\r\n// 重置查询\r\nconst resetQuery = () => {\r\n  Object.keys(queryParams).forEach(key => {\r\n    queryParams[key] = ''\r\n  })\r\n  dateRange.value = []\r\n  currentPage.value = 1\r\n  fetchDoctorData()\r\n}\r\n\r\n// 分页处理\r\nconst handlePageChange = (val) => {\r\n  currentPage.value = val\r\n  fetchDoctorData()\r\n}\r\n\r\nconst handleSizeChange = (val) => {\r\n  pageSize.value = val\r\n  currentPage.value = 1\r\n  fetchDoctorData()\r\n}\r\n\r\n// 辅助函数\r\nconst formatCurrency = (value) => {\r\n  return parseFloat(value).toFixed(2).replace(/\\d(?=(\\d{3})+\\.)/g, '$&,')\r\n}\r\n\r\nconst getRiskTagType = (score) => {\r\n  if (score >= 80) return 'danger'\r\n  if (score >= 60) return 'warning'\r\n  if (score >= 40) return 'success'\r\n  return 'info'\r\n}\r\n\r\nconst getProgressColor = (score) => {\r\n  if (score >= 80) return '#F56C6C'\r\n  if (score >= 60) return '#E6A23C'\r\n  if (score >= 40) return '#67C23A'\r\n  return '#909399'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 2000px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.title {\r\n  font-size: 1.5rem;\r\n  font-weight: 500;\r\n  color: var(--el-color-primary);\r\n}\r\n\r\n.search-form {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.pagination {\r\n  margin-top: 20px;\r\n  justify-content: flex-end;\r\n}\r\n\r\n.risk-assessment {\r\n  padding: 10px;\r\n}\r\n\r\n.doctor-info {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.doctor-info h3 {\r\n  margin: 0 0 10px 0;\r\n  color: #303133;\r\n}\r\n\r\n.doctor-info p {\r\n  margin: 0;\r\n  color: #606266;\r\n}\r\n\r\n.risk-indicators {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.indicator-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.indicator-body {\r\n  margin-top: 10px;\r\n}\r\n\r\n.indicator-body p {\r\n  margin: 10px 0 0 0;\r\n  font-size: 0.9rem;\r\n  color: #606266;\r\n  text-align: center;\r\n}\r\n\r\n.risk-trend {\r\n  width: 100%;\r\n  h