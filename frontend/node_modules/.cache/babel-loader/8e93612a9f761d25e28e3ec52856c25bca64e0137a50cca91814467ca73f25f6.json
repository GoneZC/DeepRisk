 42,\n      drugRepetitionScore: 85,\n      repetitionRate: 38,\n      anomalyPrescriptions: []\n    });\n\n    // 查询参数\n    var queryParams = reactive({\n      doctorId: '',\n      hospital: '',\n      department: '',\n      community: '',\n      startDate: '',\n      endDate: ''\n    });\n\n    // 选项数据\n    var hospitalOptions = [{\n      value: 'H001',\n      label: '第一人民医院'\n    }, {\n      value: 'H002',\n      label: '中心医院'\n    }, {\n      value: 'H003',\n      label: '妇幼保健院'\n    }, {\n      value: 'H004',\n      label: '社区医疗中心'\n    }];\n    var departmentOptions = [{\n      value: 'D001',\n      label: '内科'\n    }, {\n      value: 'D002',\n      label: '外科'\n    }, {\n      value: 'D003',\n      label: '妇科'\n    }, {\n      value: 'D004',\n      label: '儿科'\n    }, {\n      value: 'D005',\n      label: '骨科'\n    }, {\n      value: 'D006',\n      label: '神经科'\n    }];\n\n    // 初始化\n    onMounted(function () {\n      fetchDoctorData();\n    });\n\n    // 获取医生数据\n    var fetchDoctorData = /*#__PURE__*/function () {\n      var _ref2 = _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              loading.value = true;\n              try {\n                // 模拟API调用\n                setTimeout(function () {\n                  doctorList.value = generateMockDoctorData();\n                  total.value = 156;\n                  loading.value = false;\n                }, 800);\n              } catch (error) {\n                ElMessage.error('获取数据失败');\n                loading.value = false;\n              }\n            case 2:\n            case \"end\":\n              return _context.stop();\n          }\n        }, _callee);\n      }));\n      return function fetchDoctorData() {\n        return _ref2.apply(this, arguments);\n      };\n    }();\n\n    // 生成模拟数据\n    var generateMockDoctorData = function generateMockDoctorData() {\n      var mockData = [];\n      for (var i = 1; i <= 10; i++) {\n        var hospitalIndex = Math.floor(Math.random() * 4);\n        var departmentIndex = Math.floor(Math.random() * 6);\n        var prescriptionCount = Math.floor(Math.random() * 200) + 50;\n        var totalCost = Math.random() * 100000 + 10000;\n        mockData.push({\n          doctorId: 'DOC' + String(i).padStart(4, '0'),\n          doctorName: '张医生' + i,\n          hospitalCode: hospitalOptions[hospitalIndex].value,\n          hospitalName: hospitalOptions[hospitalIndex].label,\n          department: departmentOptions[departmentIndex].label,\n          community: '社区' + (Math.floor(Math.random() * 5) + 1),\n          prescriptionCount: prescriptionCount,\n          totalCost: totalCost,\n          avgCostPerPrescription: totalCost / prescriptionCount,\n          lastPrescriptionDate: '2023-12-' + String(Math.floor(Math.random() * 30) + 1).padStart(2, '0'),\n          riskScore: Math.floor(Math.random() * 100)\n        });\n      }\n      return mockData;\n    };\n\n    // 处理风险评估\n    var handleRiskAssessment = function handleRiskAssessment(doctor) {\n      selectedDoctor.value = doctor;\n      riskDialogVisible.value = true;\n\n      // 模拟异常处方数据\n      riskData.anomalyPrescriptions = [];\n      var anomalyTypes = ['重复用药', '超量开药', '不合理用药', '过度检查'];\n      for (var i = 1; i <= 5; i++) {\n        var type = anomalyTypes[Math.floor(Math.random() * anomalyTypes.length)];\n        riskData.anomalyPrescriptions.push({\n          prescriptionId: 'P' + String(Math.floor(Math.random() * 10000)).padStart(5, '0'),\n          patientId: 'PAT' + String(Math.floor(Math.random() * 10000)).padStart(5, '0'),\n          prescriptionDate: '2023-12-' + String(Math.floor(Math.random() * 30) + 1).padStart(2, '0'),\n          totalCost: Math.random() * 1000 + 100,\n          anomalyType: type,\n          confidence: Math.random() * 0.5 + 0.5,\n          description: type + '：医生在短期内为同一患者重复开具相同或类似药品，超出合理用量范围。'\n        });\n      }\n\n      // 初始化趋势图表\n      setTimeout(function () {\n        initTrendChart();\n      }, 100);\n    };\n\n    // 初始化趋势图\n    var initTrendChart = function initTrendChart() {\n      var chartDom = document.querySelector('.risk-trend');\n      if (!chartDom) return;\n      trendChartInstance.value = echarts.init(chartDom);\n      var months = ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月'];\n      var riskScores = Array(12).fill(0).map(function () {\n        return Math.floor(Math.random() * 40) + 60;\n      });\n      var avgScores = Array(12).fill(0).map(function () {\n        return Math.floor(Math.random() * 20) + 40;\n      });\n      var option = {\n        title: {\n          text: '医生风险评分趋势',\n          left: 'center'\n        },\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {\n            type: 'shadow'\n          }\n        },\n        legend: {\n          data: ['风险评分', '同科室平均'],\n          bottom: '0%'\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '10%',\n          top: '15%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'category',\n          data: months\n        },\n        yAxis: {\n          type: 'value',\n          min: 0,\n          max: 100\n        },\n        series: [{\n          name: '风险评分',\n          type: 'line',\n          data: riskScores,\n          itemStyle: {\n            color: '#F56C6C'\n          },\n          lineStyle: {\n            width: 3\n          }\n        }, {\n          name: '同科室平均',\n          type: 'line',\n          data: avgScores,\n          itemStyle: {\n            color: '#409EFF'\n          },\n          lineStyle: {\n            width: 3,\n            type: 'dashed'\n          }\n        }]\n      };\n      trendChartInstance.value.setOption(option);\n    };\n\n    // 处理人工审核\n    var handleManualReview = function handleManualReview(doctor) {\n      ElMessage.info(\"\\u6B63\\u5728\\u51C6\\u5907\".concat(doctor.doctorName || doctor.doctorId, \"\\u7684\\u4EBA\\u5DE5\\u5BA1\\u6838\\u8868\\u5355...\"));\n      // 这里可以跳转到详细的审核页面或打开审核对话框\n    };\n\n    // 查询处理\n    var handleSearch = function handleSearch() {\n      if (dateRange.value && dateRange.value.length === 2) {\n        queryParams.startDate = dateRange.value[0];\n        queryParams.endDate = dateRange.value[1];\n      }\n      currentPage.value = 1;\n      fetchDoctorData();\n    };\n\n    // 重置查询\n    var resetQuery = function resetQuery() {\n      Object.keys(queryParams).forEach(function (key) {\n        queryParams[key] = '';\n      });\n      dateRange.value = [];\n      currentPage.value = 1;\n      fetchDoctorData();\n    };\n\n    // 分页处理\n    var handlePageChange = function handlePageChange(val) {\n      currentPage.value = val;\n      fetchDoctorData();\n    };\n    var handleSizeChange = function handleSizeChange(val) {\n      pageSize.value = val;\n      currentPage.value = 1;\n      fetchDoctorData();\n    };\n\n    // 辅助函数\n    var formatCurrency = function formatCurrency(value) {\n      return parseFloat(value).toFixed(2).replace(/\\d(?=(\\d{3})+\\.)/g, '$&,');\n    };\n    var getRiskTagType = function getRiskTagType(score) {\n      if (score >= 80) return 'danger';\n      if (score >= 60) return 'warning';\n      if (score >= 40) return 'success';\n      return 'info';\n    };\n    var getProgressColor = function getProgressColor(score) {\n      if (score >= 80) return '#F56C6C';\n      if (score >= 60) return '#E6A23C';\n      if (score >= 40) return '#67C23A';\n      return '#909399';\n    };\n    var __returned__ = {\n      loading: loading,\n      currentPage: currentPage,\n      pageSize: pageSize,\n      total: total,\n      dateRange: dateRange,\n      doctorList: doctorList,\n      trendChartInstance: trendChartInstance,\n      riskDialogVisible: riskDialogVisible,\n      selectedDoctor: selectedDoctor,\n      riskData: riskData,\n      queryParams: queryParams,\n      hospitalOptions: hospitalOptions,\n      departmentOptions: departmentOptions,\n      fetchDoctorData: fetchDoctorData,\n      generateMockDoctorData: generateMockDoctorData,\n      handleRiskAssessment: handleRiskAssessment,\n      initTrendChart: initTrendChart,\n      handleManualReview: handleManualReview,\n      handleSearch: handleSearch,\n      resetQuery: resetQuery,\n      handlePageChange: handlePageChange,\n      handleSizeChange: handleSizeChange,\n      formatCurrency: formatCurrency,\n      getRiskTagType: getRiskTagType,\n      getProgressColor: getProgressColor,\n      ref: ref,\n      reactive: reactive,\n      onMounted: onMounted,\n      get Search() {\n        return Search;\n      },\n      get Refresh() {\n        return Refresh;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get echarts() {\n        return echarts;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","Search","Refresh","ElMessage","echarts","loading","currentPage","pageSize","total","dateRange","doctorList","trendChartInstance","riskDialogVisible","selectedDoctor","riskData","costAnomalyScore","costDifference","prescriptionFrequencyScore","monthlyPrescriptions","drugRepetitionScore","repetitionRate","anomalyPrescriptions","queryParams","doctorId","hospital","department","community","startDate","endDate","hospitalOptions","value","label","departmentOptions","fetchDoctorData","_ref2","_asyncToGenerator","_regeneratorRuntime","mark","_callee","wrap","_callee$","_context","prev","next","setTimeout","generateMockDoctorData","error","stop","apply","arguments","mockData","i","hospitalIndex","Math","floor","random","departmentIndex","prescriptionCount","totalCost","push","String","padStart","doctorName",